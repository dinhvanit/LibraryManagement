<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uet.librarymanagementsystem.controllers.admin.AddDocumentController">
   <children>
      <VBox fx:id="VBoxOfTextField" alignment="CENTER" fillWidth="false" prefHeight="342.0" prefWidth="282.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <children>
              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="ADD BOOK">
                  <font>
                      <Font size="25.0" />
                  </font>
              </Text>
              <TextField fx:id="documentName" prefWidth="226.0" promptText="Document Name" VBox.vgrow="NEVER">
                  <VBox.margin>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </VBox.margin>
              </TextField>
              <TextField fx:id="documentTitle" prefWidth="226.0" promptText="Document Title" VBox.vgrow="NEVER">
                  <VBox.margin>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </VBox.margin>
              </TextField>
              <TextField fx:id="author" prefWidth="226.0" promptText="Author" VBox.vgrow="NEVER">
                  <VBox.margin>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </VBox.margin>
              </TextField>
              <TextField fx:id="quantity" prefWidth="226.0" promptText="Quantity" VBox.vgrow="NEVER">
                  <VBox.margin>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </VBox.margin>
              </TextField>
              <TextField fx:id="documentId" prefWidth="226.0" promptText="Document Id" VBox.vgrow="NEVER">
                  <opaqueInsets>
                      <Insets />
                  </opaqueInsets>
                  <VBox.margin>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </VBox.margin>
              </TextField>
              <HBox fx:id="HBoxOfButton" prefHeight="80.0" prefWidth="226.0">
                  <children>
                      <Button mnemonicParsing="false" onMouseClicked="#addOfAddBook" prefHeight="50.0" prefWidth="170.0" text="Add">
                          <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                          </HBox.margin>
                      </Button>
                      <Button mnemonicParsing="false" onMouseClicked="#cancelOfAddBook" prefHeight="50.0" prefWidth="169.0" text="Cancel">
                          <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                          </HBox.margin>
                      </Button>
                  </children>
              </HBox>
          </children>
      </VBox>
   </children>
</AnchorPane>
